sudo $EDITOR /etc/fstab

F2FS:
noatime,lazytime,compress_algorithm=zstd:3,compress_chksum,compress_cache,atgc,gc_merge,inline_xattr,inline_data,inline_dentry,background_gc=on,extent_cache,age_extent_cache,alloc_mode=reuse,nat_bits,whint_mode=fs-based

XFS:
noatime,lazytime,inode64,swalloc,largeio,logbufs=8,logbsize=256k

btrfs:
noatime,lazytime,space_cache=v2,ssd,discard=async,commit=120,compress=zstd:3

Ext4:
noatime,lazytime,commit=120,discard=async,data=writeback,journal_async_commit,nobh,barrier=0

# Check
sudo tune2fs -l /dev/nvme0n1p2
sudo tune2fs -O fast_commit /dev/nvme0n1p2
