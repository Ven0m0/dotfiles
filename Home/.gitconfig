[credential "https://github.com"]
  helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[user]
  name = Ven0m0
  email = ven0m0.wastaken@gmail.com
[core]
  excludesfile = ~/.gitignore
  attributesfile = ~/.gitattributes
  pager = delta
  whitespace = fix,-indent-with-non-tab,traili ng-space,space-before-tab,cr-at-eol
  editor = $EDITOR
  autocrlf = input
  eol = native
  compression = 6
  fsyncMethod = batch
  deltaBaseCacheLimit = 1g
  untrackedCache = true
  symlinks = true
  commitGraph = true
  preloadindex = true
  abbrev = 12
  fileMode = true
  multiPackIndex = true
[init]
  defaultBranch = main
[alias]
  st = status -sb
  s = status -s
  unstage = reset HEAD --
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  last = log -1 HEAD --stat
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  df = diff
  dc = diff --cached
  ds = diff --stat
  addall = add --all
  aa = add --all
  ap = add --patch
  rbmain = pull --rebase origin main
  pushf = push --force-with-lease --force-if-includes
  commitm = commit -m
  amend = commit --amend --no-edit
  amendc = commit --amend
  fixup = commit --fixup
  undo = reset --soft HEAD~1
  discard = restore --
  cleanup = !git branch --merged | grep -v '\\*\\|main\\|master\\|develop' | xargs -n 1 git branch -d
  cleanupf = !git fetch -p && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs -r git branch -D
  sub = submodule update --init --recursive --remote
  dot = !git --git-dir=$HOME/.dotfiles --work-tree=$HOME
  root = rev-parse --show-toplevel
  aliases = config --get-regexp '^alias\\.'
  stashes = stash list
  conflicts = diff --name-only --diff-filter=U
  new = !sh -c 'git log $1@{1}. .$1@{0} "$@"'
  overview = log --all --oneline --no-merges
  today = log --since=midnight --author=\"$(git config user.name)\" --oneline
  contributors = shortlog -sn --no-merges
  hub = "!xdg-open $(git remote get-url origin 2>/dev/null) >/dev/null 2>&1"
[color]
  ui = auto
  branch = auto
  diff = auto
  status = auto
[push]
  default = current
  autoSetupRemote = true
  recurseSubmodules = on-demand
  followTags = true
  useForceIfIncludes = true
[pull]
  ff = only
  rebase = true
[fetch]
  prune = true
  pruneTags = true
  parallel = 0
  recurseSubmodules = on-demand
  writeCommitGraph = true
  negotiationAlgorithm = skipping
[clone]
  filterSubmodules = true
[pack]
  threads = 0
  compression = 6
  indexVersion = 2
  useSparse = true
  useBitmaps = true
  writeBitmapHashCache = true
  deltaCacheSize = 512m
  window = 128
  depth = 256
[rebase]
  autoSquash = true
  autoStash = true
  updateRefs = true
  missingCommitsCheck = warn
  abbreviateCommands = true
  backend = merge
[gc]
  auto = 256
  autoPackLimit = 50
  packRefs = true
  writeCommitGraph = true
  aggressive = false
  aggressiveDepth = 250
  aggressiveWindow = 250
  pruneExpire = 7.days.ago
  reflogExpire = 30.days.ago
  reflogExpireUnreachable = 7.days.ago
  repackFilter = blob:none
[index]
  threads = 0
  version = 4
  skipHash = true
[submodule]
  fetchJobs = 0
[checkout]
  guess = true
  workers = 0
[rebase]
  autoStash = true
[rerere]
  enabled = true
  autoupdate = true
[receive]
  autogc = true
  shallowUpdate = true
[http]
  version = HTTP/2
  postBuffer = 104857600
  followRedirects = true
  maxRequests = 10
[transfer]
  fsckObjects = true
[help]
  autoCorrect = prompt
[delta]
  hyperlinks = true
  features = decorations
  line-numbers = true
  navigate = true
  side-by-side = false
  syntax-theme = Monokai Extended
[diff]
  tool = vscode
  algorithm = histogram
  colorMoved = zebra
  colorMovedWS = allow-indentation-change
  renames = copies
  context = 5
  suppressBlankEmpty = true
  compactionHeuristic = true
  mnemonicprefix = true
  relative = true
  submodule = log
[difftool]
  prompt = false
[difftool "vscode"]
  cmd = vscodium --wait --diff $LOCAL $REMOTE
[diff-so-fancy]
  markEmptyLines = false
  stripLeadingSymbols = true
[icdiff]
  options = --line-numbers
[merge]
  conflictStyle = zdiff3
  tool = vscodium
  stat = true
  summary = true
  ff = false
  autoStash = true
[mergetool]
  prompt = false
  keepBackup = false
  hideResolved = true
[mergetool "vscode"]
  cmd = vscodium --wait $MERGED
  useAutoMerge = true
[mergetool "meld"]
  useAutoMerge = true
[gui]
  fastCopyBlame = true
  pruneDuringFetch = true
  encoding = utf-8
[feature]
  manyFiles = true
  experimental = true
[status]
  short = false
  submoduleSummary = true
  showUntrackedFiles = all
  aheadBehind = true
  showStash = true
  relativePaths = true
[commit]
  verbose = true
  gpgsign = false
[branch]
  sort = -committerdate
  autoSetupMerge = simple
[log]
  date = iso
  abbrevCommit = true
  follow = true
[apply]
  whitespace = fix
[credential]
  helper = store
[filter "lfs"]
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
[tag]
  sort = version: refname
  gpgSign = false
[protocol]
  version = 2
[protocol "file"]
  allow = always
[maintenance]
  strategy = incremental
  auto = true
[maintenance "prefetch"]
  enabled = true
[maintenance "gc"]
  enabled = true
[maintenance "loose-objects"]
  enabled = true
[maintenance "incremental-repack"]
  enabled = true
[maintenance "pack-refs"]
  enabled = true
[maintenance "commit-graph"]
  enabled = true
[maintenance "reflog-expire"]
  enabled = true
[maintenance "rerere-gc"]
  enabled = true
[maintenance "worktree-prune"]
  enabled = true
[gpg]
  program = gpg
[safe]
  directory = *
[remote "origin"]
  prune = true
