{
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "general": {
    "previewFeatures": true,
    "sessionRetention": {
      "enabled": true
    },
    "preferredEditor": "vscode",
    "disableAutoUpdate": true
  },
  "output": {
    "format": "text"
  },
  "ui": {
    "hideTips": true,
    "hideBanner": true,
    "footer": {
      "hideSandboxStatus": true
    },
    "showMemoryUsage": true,
    "hideFooter": true
  },
  "context": {
    "loadMemoryFromIncludeDirectories": true
  },
  "model": {
    "skipNextSpeakerCheck": true
  },
  "tools": {
    "autoAccept": true
  },
  "mcpServers": {
    "read-website-fast": {
      "command": "bunx",
      "args": ["-y", "@just-every/mcp-read-website-fast"]
    },
    "sequential-thinking": {
      "command": "bunx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "fast-filesystem": {
      "command": "bunx",
      "args": ["-y", "fast-filesystem-mcp"]
    },
    "memory": {
      "command": "bunx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "mcp-server-commands": {
      "command": "bunx",
      "args": ["-y", "mcp-server-commands"]
    },
    "think-tool": {
      "command": "bunx",
      "args": ["-y", "think-tool-mcp"]
    },
    "mcp-tasks": {
      "command": "bunx",
      "args": ["-y", "mcp-tasks"]
    },
    "Github": {
      "url": "https://api.githubcopilot.com/mcp/",
      "type": "http"
    },
    "promptboost": {
      "url": "https://promptboost.dev/mcp",
      "type": "http"
    },
    "fetch": {
      "url": "https://remote.mcpservers.org/fetch/mcp",
      "type": "http"
    },
    "playwright": {
      "url": "https://api.mcphub.com/mcp/executeautomation-mcp-playwright",
      "type": "http"
    },
    "sequential": {
      "url": "https://remote.mcpservers.org/sequentialthinking/mcp",
      "type": "http"
    },
    "basic-memory": {
      "url": "https://api.mcphub.com/mcp/basicmachines-co-basic-memory",
      "type": "http"
    }
  }
}
