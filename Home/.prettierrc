{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "printWidth": 120,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "plugins": [
    "@prettier/plugin-sh",
    "@prettier/plugin-oxc"
  ]
}
