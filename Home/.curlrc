### General ###
silent
show-error
progress-bar
# follow redirects
location
# Wait before timing out.
connect-timeout 15
create-dirs
skip-existing
fail
fail-early
netrc-optional
hsts "/home/ven0m0/.curl-hsts"

# HTTP
tls-max 1.3
http3
tcp-fastopen
tcp-nodelay
tls-earlydata
mptcp
ssl-no-revoke
doh-insecure

# Compression
compressed
compressed-ssh
#tr-encoding
parallel
#parallel-max 16

# When following a redirect, automatically set the previous URL as referer.
referer ";auto"

# Continue transfer
continue-at -

# FTP
ftp-create-dirs
ftp-method nocwd

# Use the server-provided timestamp, if any.
remote-time
remote-name-all
# remote-name
suppress-connect-headers
