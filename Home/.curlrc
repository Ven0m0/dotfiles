silent
show-error
progress-bar
location
connect-timeout 15
create-dirs
skip-existing
fail
fail-early
netrc-optional
hsts "~/.curl-hsts"

tls-max 1.3
http3
tcp-fastopen
tcp-nodelay
tls-earlydata
mptcp

# compressed
parallel

# When following a redirect, automatically set the previous URL as referer.
referer ";auto"

# Continue transfer
continue-at -

# Use the server-provided timestamp, if any.
# remote-time
# remote-name-all
suppress-connect-headers
