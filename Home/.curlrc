# curl configuration file
# https://curl.se/docs/manpage.html

# Display options
silent
show-error
progress-bar
write-out "\n"

# Behavior
location
max-redirs = 10
retry = 3
retry-delay = 2
retry-max-time = 60
connect-timeout = 15
max-time = 300
create-dirs
skip-existing
fail
fail-early
netrc-optional

# Security
tls-max 1.3
proto "=https,http,ftp,ftps"
proto-redir "=https,http"
proto-default https

# Performance
#http3
http2
tcp-fastopen
tcp-nodelay
tls-earlydata
mptcp
compressed
parallel
parallel-max = 10

# HTTP Headers
user-agent = "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
referer ";auto"

# Transfer
continue-at -
remote-time
suppress-connect-headers
