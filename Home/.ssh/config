Host *
  Protocol 2
  Compression no
  TCPKeepAlive yes
  ServerAliveInterval 60
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%n:%p
  ControlPersist 15m
  ForwardAgent no
  Ciphers aes128-gcm@openssh.com,aes256-gcm@openssh.com,chacha20-poly1305@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
  SendEnv COLORTERM LANG LC_*
  SetEnv TERM="xterm-256color"
  IgnoreUnknown UseKeychain
  UseKeyChain yes
  HappyEyes yes
  UseMPTCP yes
  IPQoS ef af11
  LogLevel Error

Host pi3
  HostName 192.168.178.86
  User dietpi
  IdentityFile ~/.ssh/id_ed25519
  ControlPersist yes
  Compression no
  IPQoS ef af11
Host pi4
  HostName 192.168.178.81
  User dietpi
  IdentityFile ~/.ssh/id_git
  ControlPersist yes
  Compression no
  IPQoS ef af11

# Git
Host github.com
  Hostname ssh.github.com
  Port 443
  User git
  IdentityFile ~/.ssh/id_git
  ForwardAgent yes
  Compression no
  HappyEyes yes
  IPQoS ef af11
Host gitlab.com
  Hostname ssh.gitlab.com
  Port 443
  User git
  IdentityFile ~/.ssh/id_git
  ForwardAgent yes
  Compression no
  HappyEyes yes
  IPQoS ef af11
Host aur.archlinux.org
  Compression no
  HappyEyes yes
  IPQoS ef af11
