Host *
    Protocol 2
    Compression no
    TCPKeepAlive yes
    ServerAliveInterval 60
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%n:%p
    ControlPersist 15m
    ForwardAgent no
    Ciphers aes128-gcm@openssh.com,aes256-gcm@openssh.com,chacha20-poly1305@openssh.com
    SendEnv COLORTERM LANG LC_*
    SetEnv TERM="xterm-256color"
    IgnoreUnknown UseKeychain
    UseKeyChain yes
    HappyEyes yes
    UseMPTCP yes

Host pi3
    HostName 192.168.178.86
    User dietpi
    IdentityFile ~/.ssh/id_ed25519
    ControlPersist yes
    Compression no
    IPQoS 46

Host pi4
    HostName 192.168.178.81
    User dietpi
    IdentityFile ~/.ssh/id_ed25519
    ControlPersist yes
    Compression no
    IPQoS 46

