# curl configuration file
# https://curl.se/docs/manpage.html
# Gemeral
silent
show-error
progress-bar
location
create-dirs
skip-existing
remote-time
fail
fail-early
netrc-optional
continue-at -
max-redirs = 10
retry = 3
retry-delay = 2
retry-max-time = 60
connect-timeout = 15
max-time = 300
write-out "\n"
suppress-connect-headers

# Security / Performance
tls-max 1.3
proto "=https,http,ftp,ftps"
proto-redir "=https,http"
proto-default https
http2
tcp-fastopen
tcp-nodelay
tls-earlydata
mptcp
compressed
parallel
parallel-max = 10

# HTTP Headers
user-agent = "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36 Firefox/145.0"
referer ";auto"
